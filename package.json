{
	"name": "macos-app-trial-extender",
	"version": "1.0.2",
	"private": true,
	"description": "MATE is an application that allows you to reset the trial period of native macos applications such as Final Cut Pro or Logic Pro.",
	"keywords": [
		"app",
		"apple",
		"darwin",
		"desktop",
		"dmg",
		"final-cut-pro",
		"final-cut-x",
		"js",
		"logic-pro",
		"logic-pro-x",
		"macos",
		"tauri"
	],
	"homepage": "https://github.com/angelespejo/macos-app-trial-extender",
	"bugs": {
		"url": "https://github.com/angelespejo/macos-app-trial-extender/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/angelespejo/macos-app-trial-extender"
	},
	"funding": {
		"type": "individual",
		"url": "https://github.com/sponsors/angelespejo"
	},
	"license": "GPL-3.0",
	"author": {
		"name": "Angelo",
		"email": "angelo@pigeonposse.com",
		"url": "https://github.com/angelespejo"
	},
	"type": "module",
	"scripts": {
		"add-test": "pnpx tsx ./.dovenv/add-test/index.ts",
		"build": "tauri build",
		"build:icon": "tauri icon docs/icon.png",
		"build:web": "vite build",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"co": "dovenv git commit",
		"dev": "tauri dev",
		"dev-info": "dovenv info",
		"dev:web": "vite",
		"lint": "pnpm lint:eslint && pnpm lint:rust",
		"lint:eslint": "pnpm eslint .",
		"lint:fix": "pnpm eslint . --fix && pnpm lint:rust",
		"lint:rust": "cargo fmt --manifest-path ./src-tauri/Cargo.toml",
		"prepare": "dovenv --silent git husky .dev/husky",
		"preview:web": "vite preview",
		"push": "dovenv git push --skip-update",
		"readme": "dovenv readme --silent",
		"release": "pnpm release-it",
		"tauri": "tauri $@"
	},
	"dependencies": {
		"@tauri-apps/api": "2.8.0",
		"@tauri-apps/plugin-dialog": "2.4.0",
		"@tauri-apps/plugin-fs": "2.4.2",
		"@tauri-apps/plugin-log": "~2.7.0",
		"@tauri-apps/plugin-notification": "2.3.1",
		"@tauri-apps/plugin-os": "2.3.1",
		"@tauri-apps/plugin-shell": "2.3.1",
		"@tauri-apps/plugin-store": "2.4.0",
		"@tauri-apps/plugin-updater": "2.9.0",
		"svelte-ripple-action": "2.0.0"
	},
	"devDependencies": {
		"@ascii-kit/font": "~0.0.6",
		"@ascii-kit/font-ansi--shadow": "~0.0.6",
		"@dovenv/core": "~1.6.1",
		"@dovenv/eslint-config": "~1.6.1",
		"@dovenv/lint": "~1.6.1",
		"@dovenv/repo": "~1.6.1",
		"@iconify-json/fa7-brands": "~1.2.0",
		"@iconify-json/fa7-solid": "~1.2.1",
		"@inlang/paraglide-js": "~2.3.2",
		"@release-it/bumper": "7.0.5",
		"@svaio/unocss": "0.0.11",
		"@sveltejs/adapter-static": "3.0.9",
		"@sveltejs/kit": "~2.42.1",
		"@tauri-apps/cli": "2.8.4",
		"@types/node": "~24.5.1",
		"acorn": "8.15.0",
		"acorn-walk": "8.3.4",
		"auto-changelog": "2.5.0",
		"eslint": "~9.35.0",
		"internal-ip": "8.0.1",
		"release-it": "19.0.4",
		"svelte": "~5.38.10",
		"svelte-check": "~4.3.1",
		"tslib": "~2.8.1",
		"typescript": "~5.9.2",
		"vite": "~7.1.5"
	},
	"packageManager": "pnpm@10.17.0",
	"engines": {
		"node": ">=20.0.0",
		"pnpm": ">=8.10.0"
	},
	"extra": {
